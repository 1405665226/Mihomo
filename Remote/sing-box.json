        "server": "https://dns.google/dns-query",
        "domain_resolver": "dns_resolver",
        "detour": "🚀 节点选择"
      },
      {
        "tag": "Direct DNS",
        "type": "https",
        "server": "https://dns.alidns.com/dns-query",
        "domain_resolver": "dns_resolver"
      },
      {
        "tag": "dns_resolver",
        "type": "https",
        "server": "223.5.5.5"
      },
      {
        "tag": "fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "Direct DNS"
      },
      {
        "clash_mode": "global",
        "server": "Proxy DNS"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": "geosite-cn",
        "server": "fakeip"
      },
      {
        "rule_set": "geosite-cn",
        "server": "Direct DNS"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "server": "Proxy DNS"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip"
      }
    ],
    "independent_cache": true,
    "strategy": "prefer_ipv4"
  },
